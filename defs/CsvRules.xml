  <Rule>
    <Ident>GunmaBank</Ident>
    <Name>群馬銀行</Name>
    <BankId>0128</BankId>
    <FirstLine>"取扱日付","お支払金額","お預り金額","取引区分","残高","摘要","メモ"</FirstLine>
